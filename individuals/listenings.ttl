<#sehnsucht>
  a owl:Class ;
  rdfs:subClassOf <http://purl.obolibrary.org/obo/MFOEM_000001>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000117> ;
    owl:someValuesFrom <http://purl.obolibrary.org/obo/MFOEM_000208>
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.obolibrary.org/obo/BFO_0000117> ;
    owl:someValuesFrom [
      a owl:Class ;
      owl:intersectionOf (
       <http://purl.obolibrary.org/obo/MFOEM_000002>
       _:genid6
     )
    ]
  ];
  mfoem:BFO_000011 <http://purl.obolibrary.org/obo/MFOEM_000122>;
  mfoem:IAO_0000115 "An extremely negative emotion translated from German as \"longing\", \"desire\", \"yearning\", or \"craving\". Some psychologists use the word to represent thoughts and feelings about all facets of life that are unfinished or imperfect, paired with a yearning for ideal alternative experiences." ;
  mfoem:MFOEM_000165 "Sehnsucht" ;
  foaf:name "ciao";
  rdfs:label "Sehnsucht" .

_:genid6
  a owl:Restriction ;
  owl:onProperty mfoem:MFOEM_000213 ;
  owl:someValuesFrom mfoem:MFOEM_000106 .
	
